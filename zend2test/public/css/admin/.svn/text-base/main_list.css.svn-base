@CHARSET "ISO-8859-1";

/********************************************************************************
TOOLTIP FOR USER CAREER
*********************************************************************************/

#tooltip_career
{
	position:absolute;
	border:1px solid #333;
	background:#f7f5d1;
	padding:2px 5px;
	color:#333;
	display:none;
	z-index: 10000;
}

div.tooltip_career_image
{
	float: left;
	padding: 2px;
	width: 130px;
}

div.tooltip_career_info
{
	margin-left: 10px;
	float: left;
	width: 300px;
	
}

div.tooltip_career_info label
{
	font-weight: bold;
	width: 100px;
}

td a.user-career-download{
	cursor: pointer;
	background: url("/images/admin/download.png") no-repeat center;
	width: 20px;
	height: 30px;
	display: block;
}

td a.user-career-download:hover
{
		background: url("/images/admin/download-active.png") no-repeat center;
}

/********************************************************************************
	PAGING CONTROL
*********************************************************************************/

#page_control
{
	display: block;
    height: 30px;
    width: 100%;
}
#page_control #temp_image img
{
	width: 50px;
}
#items_page_control
{
	border: 1px solid #CCCCCC;
    border-radius: 4px 4px 4px 4px;
    float: left;
    margin-right: 10px;
    padding: 3px;
}
#items_page_control .bg_page_control
{
	background: url("/images/admin/bg-button-green.gif") repeat-x scroll left top #459300 !important;
    border: 1px solid #459300 !important;
    border-radius: 4px 4px 4px 4px;
    color: #FFFFFF !important;
    cursor: pointer;
    display: inline-block;
    font-size: 12px !important;
    padding: 9px 7px !important;
}
#paging_controls
{
	border: 1px solid #CCCCCC;
    border-radius: 4px 4px 4px 4px;
    float: right;
    margin-right: 10px;
    padding: 3px;
}
#paging_controls span
{
	border: 1px solid #DDDDDD;
    border-radius: 4px 4px 4px 4px;
    margin-left: 8px;
    padding: 3px 6px;
}
#paging_controls span.active_current_page
{
	background:  url("/images/admin/bg-button-green.gif") repeat-x scroll left top #459300 !important;
	color: #FFFFFF !important;
}
#paging_controls a
{
	border: 1px solid #DDDDDD;
    border-radius: 4px 4px 4px 4px;
    margin-left: 8px;
    padding: 3px 6px;
}

#paging_controls .bg_paging_controls
{
	background: url("/images/admin/bg-button-green.gif") repeat-x scroll left top #459300 !important;
    border: 1px solid #459300 !important;
    border-radius: 4px 4px 4px 4px;
    color: #FFFFFF !important;
    cursor: pointer;
    display: inline-block;
    font-size: 12px !important;
    margin-left: 0;
    padding: 9px 7px !important;
}
#page_control #items_page_control .select_page_control
{
	border: 1px solid #CCCCCC;
    border-radius: 4px 4px 4px 4px;
    display: inline-block;
    padding: 5px;
}

/********************************************************************************
STATUS FOR ALL FORM 
*********************************************************************************/
.status-all-form
{
	cursor:pointer;
	display: block;
	height: 20px;
	text-indent: -99999px;
}
.status-all-form .status-off
{
	cursor:pointer;
	display: block;
	height: 20px;
	text-indent: -99999px;
	width: 62px;
	background: url("/images/admin/icon-off.png") no-repeat scroll 0 0 transparent;
}
.status-all-form .status-on
{
	cursor:pointer;
	display: block;
	height: 20px;
	text-indent: -99999px;
	width: 62px;
	background: url("/images/admin/icon-on.png") no-repeat scroll 0 0 transparent;
}

